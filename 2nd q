import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        int n1 = scanner.nextInt(); // number of customers Jeevan already has
        int n2 = scanner.nextInt(); // number of new customers added today
        int totalCustomers = n1 + n2; // total number of customers
        System.out.println(totalCustomers);
        scanner.close();
    }
}
